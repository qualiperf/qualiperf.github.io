	keywords        = {Actin Cytoskeleton, metabolism; Adherens Junctions, metabolism; Animals; Blotting, Western; Cadherins, metabolism; DNA-Binding Proteins, genetics; Fluorescent Antibody Technique; Immunohistochemistry; Liver, metabolism; Liver Diseases, metabolism, pathology; Mice; Mice, Knockout; Models, Biological; Phosphoenolpyruvate Carboxykinase (ATP), metabolism; beta Catenin, metabolism},
	nlm-id          = {9705402},
