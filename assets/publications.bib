	keywords        = {Animals; Fatty Acids, metabolism; Humans; Liver, metabolism, pathology; Models, Biological; Non-alcoholic Fatty Liver Disease, metabolism, pathology; Triglycerides, metabolism; fatty acid uptake; liver metabolism; metabolic zonation; non-alcoholic fatty liver diseases; steatosis},
	nlm-id          = {101217269},
