 title = {In vivo assessment of the hepatic microcirculation after mesenterico-portal bypass (REX-shunt) using orthogonal polarization spectral imaging},
 pages = {1339--1345},
 volume = {30},
 number = {9},
 journal = {Liver international : official journal of the International Association for the Study of the Liver},
 doi = {10.1111/j.1478-3231.2010.02311.x},
 file = {http://www.ncbi.nlm.nih.gov/pubmed/20666991}
